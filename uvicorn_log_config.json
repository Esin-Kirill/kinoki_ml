{
    "version": 1, 
    "disable_existing_loggers": false, 
    "formatters": {
        "default": {
            "()": "uvicorn.logging.DefaultFormatter", 
            "fmt": "[%(asctime)s: %(levelname)s] %(message)s", 
            "use_colors": null
        }
    }, 
    "handlers": {
        "file": {
            "formatter": "default", 
            "class": "logging.FileHandler", 
            "filename": "filename.log"
        },
        "error": {
            "formatter": "default", 
            "class": "logging.StreamHandler", 
            "stream": "ext://sys.stderr"
        }, 
        "console": {
            "formatter": "default", 
            "class": "logging.StreamHandler", 
            "stream": "ext://sys.stdout"
        }
    }, 
    "loggers": {
        "uvicorn": {
            "handlers": ["file", "error"], 
            "level": "INFO", 
            "propagate": false
        }, 
        "uvicorn.error": {
            "level": "INFO"
        }, 
        "uvicorn.access": {
            "handlers": ["file", "console"],
            "level": "INFO", 
            "propagate": false
        }
    }
}
